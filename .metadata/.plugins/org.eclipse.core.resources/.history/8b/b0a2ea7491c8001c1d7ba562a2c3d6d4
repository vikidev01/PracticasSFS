package com.clase1;

public class Clase1 {

	public static void main(String[] args) {
		int x=3, y=2;
		int suma = x+y;
		System.out.println("La suma es: "+suma);

		int h= 0;
		if(suma==1) {
			System.out.println("La suma es: 1");
		}else if(suma==2) {
			System.out.println("La suma es: 2");
		}else if(suma==3) {
			System.out.println("La suma es: 3");
		}else {
			System.out.println("La suma es: "+suma);
		}
	}

}
